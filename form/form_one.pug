                        //real function
                        input#rfunc(type="hidden" value="-Math.sin(Math.PI*x*y)*Math.sin(Math.PI*x*y)")

                        input#rm1(type="hidden" value="Math.sin(Math.PI*y)")
                        input#rm2(type="hidden" value="x - x*x")
                        input#rm3(type="hidden" value="Math.sin(Math.PI*y)")
                        input#rm4(type="hidden" value="x - x*x")

