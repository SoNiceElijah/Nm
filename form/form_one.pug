                        input#rb(type="hidden" value="0")
                        input#rc(type="hidden" value="1")
                        input#rd(type="hidden" value="1")
                        input#rtype(type="hidden", value="real")

                        //test function
                        input#efunc(type="hidden" value=`
                            -((Math.exp(Math.pow(Math.sin(Math.PI*x*y),2)) *
                            ((Math.PI*y)*(Math.PI*y)) * 
                            ((Math.pow(Math.sin(2 * Math.PI * x * y),2)) + 2 * Math.cos(2 * Math.PI * x *y)))
                            +
                            (Math.exp(Math.pow(Math.sin(Math.PI*y*x),2)) *
                            ((Math.PI*x)*(Math.PI*x)) * 
                            ((Math.pow(Math.sin(2 * Math.PI * y * x),2)) + 2 * Math.cos(2 * Math.PI * y *x))))                      
                        `)

                        input#em1(type="hidden" value="Math.exp(Math.pow(Math.sin(Math.PI*x*y),2))")
                        input#em2(type="hidden" value="Math.exp(Math.pow(Math.sin(Math.PI*x*y),2))")
                        input#em3(type="hidden" value="Math.exp(Math.pow(Math.sin(Math.PI*x*y),2))")
                        input#em4(type="hidden" value="Math.exp(Math.pow(Math.sin(Math.PI*x*y),2))")

                        input#ea(type="hidden" value="0")
                        input#eb(type="hidden" value="0")
                        input#ec(type="hidden" value="1")
                        input#ed(type="hidden" value="1")

                        input#etype(type="hidden", value="test")
                        input#ereal(type="hidden", value="Math.exp(Math.pow(Math.sin(Math.PI*x*y),2))")

